{
  "name": "@myonlinestore/percona-writer-elactor-operator",
  "homepage": "https://github.com/MyOnlineStore/percona-writer-elactor-operator",
  "description": "K8s operator for creating a single writer endpoint for a percona cluster created with the pxc-operator",
  "version": "0.1.0",
  "license": "GNU GPLv3",
  "private": true,
  "dependencies": {
    "express": "^4.17.1",
    "prom-client": "^12.0.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "if-env APP_ENV=production || if-env APP_ENV=staging && npm run start:prod || npm run start:dev",
    "start:dev": "./node_modules/.bin/ts-node-dev --project tsconfig.json  --poll --interval 1000 --respawn --transpileOnly -- src/index.ts",
    "start:prod": "node ./dist/index.js"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": "5.x"
  }
}
